{
  "extends": "../../tslint.json",
  "rules": {
    "no-non-null-assertion": false,
    "variable-name": false,
    "no-bitwise": false,
    "directive-selector": [
      true,
      "attribute",
      "lib",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "lib",
      "kebab-case"
    ]
  }
}
